ctedRoute>\n          }\n        />\n\n        <Route\n          path=\"/dashboard-orders\"\n          element={\n            <SellerProtectedRoute>\n              <ShopAllOrders />\n            </SellerProtectedRoute>\n          }\n        />\n\n        <Route\n          path=\"/dashboard-refunds\"\n          element={\n            <SellerProtectedRoute>\n              <ShopAllRefunds />\n            </SellerProtectedRoute>\n          }\n        />\n\n        <Route\n          path=\"/order/:id\"\n          element={\n            <SellerProtectedRoute>\n              <ShopOrderDetails />\n            </SellerProtectedRoute>\n          }\n        />\n\n        <Route\n          path=\"/dashboard-products\"\n          element={\n            <SellerProtectedRoute>\n              <ShopAllProducts />\n            </SellerProtectedRoute>\n          }\n        />\n\n        <Route\n          path=\"/dashboard-withdraw-money\"\n          element={\n            <SellerProtectedRoute>\n              <ShopWithDrawMoneyPage />\n            </SellerProtectedRoute>\n          }\n        />\n\n        <Route\n          path=\"/dashboard-messages\"\n          element={\n            <SellerProtectedRoute>\n              <ShopInboxPage />\n            </SellerProtectedRoute>\n          }\n        />\n\n        <Route\n          path=\"/dashboard-create-event\"\n          element={\n            <SellerProtectedRoute>\n              <ShopCreateEvents />\n            </SellerProtectedRoute>\n          }\n        />\n        <Route\n          path=\"/dashboard-events\"\n          element={\n            <SellerProtectedRoute>\n              <ShopAllEvents />\n            </SellerProtectedRoute>\n          }\n        />\n        <Route\n          path=\"/dashboard-coupouns\"\n          element={\n            <SellerProtectedRoute>\n              <ShopAllCoupouns />\n            </SellerProtectedRoute>\n          }\n        />\n\n        {/* Admin Routes */}\n        <Route\n          path